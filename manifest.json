{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.1",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "views/popup.html",
    "default_icon": {
      "16": "images/1.png",
      "32": "images/1.png",
      "48": "images/1.png",
      "128": "images/1.png"
    }
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content_script.js"]
    }
  ],
  "icons": {
    "48": "images/1.png"
  }
}
